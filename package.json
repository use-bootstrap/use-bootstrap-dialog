{
  "name": "use-bootstrap-dialog",
  "type": "module",
  "version": "1.0.0",
  "description": "A JavaScript library for generating dialogs using Bootstrap modals",
  "license": "MIT",
  "homepage": "https://use-bootstrap-dialog.js.org",
  "repository": "https://github.com/use-bootstrap/use-bootstrap-dialog",
  "bugs": {
    "url": "https://github.com/use-bootstrap/use-bootstrap-dialog/issues"
  },
  "keywords": [
    "modal",
    "dialog",
    "alert",
    "confirm",
    "prompt",
    "bootstrap modal",
    "bootstrap dialog",
    "bootstrap alert",
    "bootstrap confirm"
  ],
  "source": "./src/lib/use-bootstrap-dialog.tsx",
  "main": "./dist/js/use-bootstrap-dialog.esm.js",
  "module": "./dist/js/use-bootstrap-dialog.esm.js",
  "types": "./dist/js/use-bootstrap-dialog.esm.d.ts",
  "libName": "UseBootstrapDialog",
  "files": [
    "dist",
    "scss"
  ],
  "browserslist": [
    ">= 0.5%",
    "last 2 major versions",
    "not dead",
    "Chrome >= 60",
    "Firefox >= 60",
    "Firefox ESR",
    "iOS >= 12",
    "Safari >= 12",
    "not Explorer <= 11"
  ],
  "scripts": {
    "dev": "astro dev --host",
    "prebuild": "rm -rf dist && tsc && eslint .",
    "build": "jalankan build:*",
    "build:js": "vite build",
    "build:js:min": "esbuild dist/js/*.js --minify --outdir=dist/js --out-extension:.js=.min.js --tsconfig-raw='{}'",
    "build:css": "sass --source-map --embed-sources src/lib:dist/css",
    "build:css:prefix": "postcss dist/css/*.css --use autoprefixer --map --replace",
    "build:css:min": "esbuild dist/css/*.css --minify --outdir=dist/css --out-extension:.css=.min.css",
    "build:docs": "astro build",
    "postbuild": "purgecss --config ./purgecss.config.cjs",
    "preview": "astro preview --host",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "devDependencies": {
    "@antfu/eslint-config": "2.21.1",
    "@astrojs/check": "0.7.0",
    "@astrojs/solid-js": "^4.4.0",
    "@fontsource/hind-siliguri": "5.0.13",
    "@fontsource/inter-tight": "5.0.20",
    "@types/node": "20.14.9",
    "astro": "4.11.1",
    "autoprefixer": "10.4.19",
    "bootstrap": "5.3.3",
    "bootstrap-esm": "1.0.0",
    "clsx": "2.1.1",
    "dts-bundle-generator": "9.5.1",
    "esbuild": "0.21.5",
    "eslint": "9.5.0",
    "jalankan": "0.0.1",
    "postcss": "8.4.38",
    "postcss-cli": "11.0.0",
    "prettier": "3.3.2",
    "purgecss": "6.0.0",
    "sass": "npm:sass-embedded@1.77.5",
    "solid-js": "^1.8.17",
    "typescript": "5.5.2",
    "vite": "5.3.1",
    "vite-plugin-dts": "3.9.1",
    "vite-plugin-solid": "2.10.2"
  }
}
